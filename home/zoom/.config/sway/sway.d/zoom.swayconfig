# vim: set filetype=swayconfig :

for_window [class="zoom"] floating enable
for_window [class="zoom" title="Zoom - Licensed Account"] floating disable
# zoom participants
for_window [class="zoom" title="Zoom Meeting"] floating disable, move to workspace 5
# zoom screen share
for_window [class="zoom" title="Zoom"] floating disable, move to workspace 5
# zoom screen share info window
for_window [class="zoom" title="zoom"] floating enable


